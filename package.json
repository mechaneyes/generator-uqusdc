  "devDependencies": {
    "@babel/core": "^7.2.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gulp-inject": "^5.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
