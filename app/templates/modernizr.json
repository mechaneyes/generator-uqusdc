{
  "classPrefix": "modernizr-",
  "options": [
    "atRule",
    "domPrefixes",
    "hasEvent",
    "html5shiv",
    "html5printshiv",
    "load",
    "mq",
    "prefixed",
    "prefixes",
    "prefixedCSS",
    "setClasses"
  ],
  "feature-detects": [
    "css/supports",
    "img/sizes",
    "img/srcset",
    "serviceworker",
    "touchevents"
  ]
}